{
    "name": "mouf/javascript.jquery.jq-bootstrap-validation",
    "description": "This package contains the jqBootstrapValidation plugin for Bootstrap. It features an installer for the Mouf framework.",
    "keywords": ["bootstrap", "validator", "jqBootstrapValidation"],
    "homepage": "http://mouf-php.com/packages/mouf/javascript.jquery.jqBootstrapValidation",
    "type": "mouf-library",
    "license": "MIT",
    "authors": [
        {
            "name": "David NÃ©grier",
            "email": "d.negrier@thecodingmachine.com",
            "homepage": "http://mouf-php.com"
        }
    ],
    "require": {
    	"php": "^8.0",
        "mouf/html.utils.weblibrarymanager": "^4.0"
    },
    "autoload": {
      "psr-4": {
        "Mouf\\Javascript\\JqBootstrapValidation\\": "src/"
      }
    },
    "extra": {
        "logo": "logo.png",
	    "mouf": {
	    	"install": [{
	    		"type": "file",
	    		"file": "install.php"
	    	}]
	    }
	},
    "config": {
        "allow-plugins": {
            "mindplay/composer-locator": true,
            "mouf/mouf-installer": true
        }
    }
}
